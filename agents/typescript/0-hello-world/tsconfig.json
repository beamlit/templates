{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules", "dist"]
}
